.tab-bar {
  display: flex;
  /* border-bottom: 1px solid #eee; */
  background: #fff;
  margin-bottom: 10px;
}
.tab-bar-item {
  flex: 1;
  text-align: center;
  padding: 10px 0;
}
.active {
  color: #ff5777;
}
.active text {
  border-bottom: 2px solid #ff5777;
  padding: 5px 10px ;
}
